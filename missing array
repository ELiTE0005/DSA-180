//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
// User function template for C++
class Solution {
  public:
    int missingNumber(vector<int>& arr) {
        
        // code here
        
        int sum = 0;
        
        for (int i = 0 ; i < arr.size(); i++) {
            
            sum = sum + arr[i];
        }
        
        int ans = (arr.size()+1)*(arr.size()+2)/2;
        
        return ans - sum;
        
        
    }
};

